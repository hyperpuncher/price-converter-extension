{
  "name": "Конвертер цен для Onliner и Kufar",
  "description": "Добавляет цены в долларах на onliner.by и kufar.by.",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": [
        "https://*.kufar.by/*"
      ],
      "js": [
        "kufar.js"
      ]
    },
    {
      "matches": [
        "https://*.onliner.by/*"
      ],
      "js": [
        "onliner.js"
      ]
    }
  ],
  "icons": {
    "16": "/images/icon16.png",
    "32": "/images/icon32.png",
    "48": "/images/icon48.png",
    "128": "/images/icon128.png"
  }
}